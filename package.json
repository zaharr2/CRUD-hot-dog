{
  "name": "vue-webpack-4",
  "version": "1.1.0",
  "description": "A Vuejs template using Webpack 4",
  "author": "Daniel Cook",
  "scripts": {
    "test": "npm run lint && npm run test:unit",
    "lint": "eslint --ext .js,.vue src",
    "dev": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "lint:fix": "eslint --ext .js,.vue src --fix",
    "prod": "webpack --config build/webpack.config.prod.js",
    "start": "webpack-dev-server --progress --config build/webpack.config.dev.js",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand --config test/jest.config.js",
    "test:unit": "jest --config test/jest.config.js"
  },
  "main": "index.js",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^6.3.4",
    "vue": "^2.6.10",
    "vue-router": "^3.1.1",
    "vuelidate": "^0.7.4",
    "vuetify": "^2.0.0",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "mini-css-extract-plugin": "^0.5.0",
    "serve": "^11.1.0",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.2",
    "vue-cli-plugin-vuetify": "^0.6.1",
    "vue-jest": "^3.0.4",
    "vue-loader": "^15.7.1",
    "vue-server-renderer": "^2.6.10",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "keywords": [
    "Vue",
    "Webpack 4"
  ],
  "license": "MIT"
}
